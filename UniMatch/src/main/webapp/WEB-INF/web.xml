<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>UniMatch</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>signup</servlet-name>
  	<servlet-class>com.unimatch.SignupServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>com.unimatch.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>signup</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <context-param>
  	<param-name>sqlurl</param-name>
  	<param-value>jdbc:mysql://localhost:3306/unimatch</param-value>
  </context-param>
  <context-param>
  	<param-name>sqluname</param-name>
  	<param-value>root</param-value>
  </context-param>
  <context-param>
  	<param-name>sqlpswd</param-name>
  	<param-value>Prabhmehar88</param-value>
  </context-param>
</web-app>